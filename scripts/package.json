  "scripts": {
    "init": "git clone git@github.com:dcaf-protocol/solana-programs.git",
    "validator:clean": "rm -rf test-ledger &&  sudo kill -9 `pidof solana-test-validator`",
    "validator": "solana-test-validator --bpf-program SwaPpA9LAaLfeLi3a68M4DjnLqgtticKg6CnyNwgAC8 ./solana-programs/deps/spl_token_swap/spl_token_swap.so > /dev/null 2>&1 &",
    "validator:wait": "yarn run validator && go run check_local_validator.go",
    "validator:deploy": "cd solana-programs && SWAP_PROGRAM_OWNER_FEE_ADDRESS=HfoTxFR1Tm6kGmWgYWD6J7YHVy1UwqSULUGVLXkJqaKN anchor test --detach > /dev/null 2>&1 &",
    "setup": "yarn run validator:deploy &&  go run check_local_validator.go && ANCHOR_WALLET=~/.config/solana/id.json yarn run ts-node setup.ts"
  }
}
